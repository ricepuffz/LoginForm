div[id=bodyWrapperWrapper] {
	width: 50%;
	background-color: #dddddd;
}

div[id=bodyWrapper] {
	width: 75%;
}

center {
	margin-top: 3vw;
}

canvas {
	background-color: #ffffff;
}

div[name=upperCanvasDiv], div[name=lowerCanvasDiv]} {
	width: 80%;
	overflow: hidden;
}

div[name=upperLeftCanvasDiv], div[name=lowerLeftCanvasDiv] {
	float: left;
	width: 50%;
}

div[name=upperRightCanvasDiv], div[name=lowerRightCanvasDiv] {
	overflow: hidden;
	width: 50%;
}

br {
	font-size: 0.7vw;
}